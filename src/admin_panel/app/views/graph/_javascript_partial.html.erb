<script type = "text/javascript">
    $j(document).ready(function(){
      $j("#slider_new_<%=count%>").slider({
          range: true,
          animate:true,
 	  a_name:'<%=count%>',
          min:0,
          max:24,
          values:[<%=start_hour%>,<%=end_hour%>],
	  slide:function(event, ui){setToolTip(ui.values[0], ui.values[1], "<%=count%>")},	
          change: function(event, ui){setValue(ui.values[0], ui.values[1], <%=app_id%>, "<%=type%>", "<%=count%>")}
          });
	});
  </script>

